		"config": "^3.2.5",
		"cross-env": "^7.0.2",
		"express": "^4.17.1",
		"express-validator": "^6.4.0",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.8.11",
		"mongoose-auto-increment": "^5.0.1",
		"node-sass": "^4.13.1",
		"nodemon": "^2.0.2"
	},
	"engines": {
		"node": "10.0.0"
	}
