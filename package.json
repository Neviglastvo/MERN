{
	"name": "pchelper",
	"version": "1.0.0",
	"description": "",
	"main": "backend/app.js",
	"scripts": {
		"dev": "node backend/app.js",
		"server": "nodemon backend/app.js",
		"client": "npm run start --prefix frontend",
		"client:install": "npm install --prefix frontend",
		"client:build": "npm run build --prefix frontend",
		"start2": "concurrently \"npm run server\" \"npm run client\"",
		"start": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\"",
		"heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install npm && run start2"
	},
